{
  "expo": {
    "name": "Inventra",
    "slug": "EXPO1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "inventra",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1155CC",
      "hideAsync": true
    },
    "updates": {
      "url": "https://u.expo.dev/a3a734b6-9f27-4308-b090-5f7524398726",
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "INVENTRA2.W",
      "googleServicesFile": "./GoogleService-info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/images/Logo White.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "hideAsync": true
      },
      "newArchEnabled": false
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1155CC",
        "foregroundImage": "./assets/images/Logo White.png"
      },
      "splash": {
        "image": "./assets/images/Logo White.png",
        "resizeMode": "contain",
        "backgroundColor": "#1155CC"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "INVENTRA2.W",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "newArchEnabled": false,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo White.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1155CC"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Inventra to access your camera to take photos of products."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Inventra to access your photos to upload product images.",
          "cameraPermission": "Allow Inventra to access your camera to take photos of products."
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "newArchEnabled": false
          },
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep class com.google.android.gms.** { *; } \n-keep class com.google.firebase.** { *; }",
            "enableMinifyInReleaseBuilds": true,
            "newArchEnabled": false,
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a3a734b6-9f27-4308-b090-5f7524398726"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
